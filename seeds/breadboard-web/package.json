{
  "name": "@google-labs/breadboard-web",
  "private": true,
  "version": "0.0.1",
  "description": "The Web runtime for Breadboard",
  "main": "./dist/src/index.js",
  "exports": {
    ".": "./dist/src/runtime.js",
    "./worker": "./dist/src/worker.js"
  },
  "types": "dist/src/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "deploy": "npm run build:vite && firebase deploy",
    "build:vite": "FORCE_COLOR=1 vite build",
    "test": "FORCE_COLOR=1 vitest --run",
    "watch": "vitest",
    "generate:graphs": "tsx src/make-graphs.ts",
    "generate:docs": "typedoc --plugin typedoc-plugin-markdown"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/google/labs-prototypes"
  },
  "files": [
    "dist/src"
  ],
  "ava": {
    "files": [
      "tests/**/*.ts"
    ],
    "workerThreads": false,
    "typescript": {
      "rewritePaths": {
        "./": "dist/"
      },
      "compile": false
    }
  },
  "keywords": [],
  "author": "Google Labs Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/google/labs-prototypes/issues"
  },
  "homepage": "https://github.com/google/labs-prototypes/tree/main/seeds/breadboard-web#readme",
  "devDependencies": {
    "@ava/typescript": "^4.0.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@google-labs/tsconfig": "*",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "ava": "^5.2.0",
    "dotenv": "^16.3.1",
    "tsx": "^3.14.0",
    "typedoc": "^0.25.1",
    "typedoc-plugin-markdown": "^3.16.0",
    "typescript": "^5.0.4",
    "vite": "^4.4.5",
    "vite-plugin-full-reload": "^1.0.5",
    "vite-plugin-watch-and-run": "^1.4.0",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@google-labs/breadboard": "*",
    "@google-labs/breadboard-ui": "*",
    "@google-labs/core-kit": "*",
    "@google-labs/palm-kit": "*",
    "@google-labs/llm-starter": "*",
    "@google-labs/node-nursery-web": "*",
    "@google-labs/pinecone-kit": "*",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.0"
  }
}
